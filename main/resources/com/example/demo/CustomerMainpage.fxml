<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="448.0" prefWidth="663.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo.CustomerMainpageController">
   <children>
      <ImageView fitHeight="448.0" fitWidth="664.0" layoutX="-1.0" layoutY="-1.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@CustomerHome.jpg" />
         </image>
      </ImageView>
      <Button layoutX="94.0" layoutY="129.0" mnemonicParsing="false" onAction="#namebutton" prefHeight="67.0" prefWidth="154.0" style="-fx-background-radius: 50;" text="Search Restaurant">
         <font>
            <Font name="System Bold Italic" size="15.0" />
         </font>
      </Button>
      <Button layoutX="443.0" layoutY="363.0" mnemonicParsing="false" onAction="#logout" prefHeight="34.0" prefWidth="79.0" style="-fx-background-radius: 50;" text="Log Out">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Button>
      <Button layoutX="375.0" layoutY="129.0" mnemonicParsing="false" onAction="#foodbutton" prefHeight="67.0" prefWidth="154.0" style="-fx-background-radius: 50;" text="Search Food">
         <font>
            <Font name="System Bold Italic" size="15.0" />
         </font>
      </Button>
      <Button layoutX="85.0" layoutY="261.0" mnemonicParsing="false" onAction="#SpecificRestaurant" prefHeight="67.0" prefWidth="210.0" style="-fx-background-radius: 50;" text="Specific Restaurant Food">
         <font>
            <Font name="System Bold Italic" size="15.0" />
         </font>
      </Button>
      <Button fx:id="cart" layoutX="391.0" layoutY="261.0" mnemonicParsing="false" onAction="#cart" prefHeight="67.0" prefWidth="154.0" style="-fx-background-radius: 50;" text="See cart">
         <font>
            <Font name="System Bold Italic" size="15.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
