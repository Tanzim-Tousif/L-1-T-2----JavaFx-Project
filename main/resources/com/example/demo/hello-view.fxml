<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="anchorpane" prefHeight="611.0" prefWidth="750.0" style="-fx-background-color: #9999FF;" stylesheets="@addFood.css" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo.HomeController">
   <children>
      <TextField fx:id="foodname" layoutX="24.0" layoutY="31.0" prefHeight="60.0" prefWidth="567.0" promptText="Enter Food Name" style="-fx-background-color: #FFFFCC; -fx-background-radius: 50;">
         <font>
            <Font name="SansSerif Bold Italic" size="18.0" />
         </font>
      </TextField>
      <AnchorPane fx:id="hiddenPane" layoutX="8.0" layoutY="137.0" prefHeight="352.0" prefWidth="726.0">
         <children>
            <TextField fx:id="categorytext" layoutX="24.0" layoutY="113.0" prefHeight="60.0" prefWidth="558.0" promptText="Enter Category Name" style="-fx-background-radius: 50; -fx-background-color: #FFFFCC;" stylesheets="@addFood.css">
               <font>
                  <Font name="SansSerif Bold Italic" size="15.0" />
               </font>
            </TextField>
            <TextField fx:id="pricetext" layoutX="24.0" prefHeight="60.0" prefWidth="308.0" promptText="Enter The Price" style="-fx-background-color: #FFFFCC; -fx-background-radius: 50;" stylesheets="@addFood.css">
               <font>
                  <Font name="SansSerif Bold Italic" size="18.0" />
               </font>
            </TextField>
            <Button fx:id="addButton" layoutX="281.0" layoutY="262.0" mnemonicParsing="false" onAction="#add" prefHeight="55.0" prefWidth="102.0" style="-fx-background-color: #FF6666; -fx-background-radius: 50;" styleClass="inbutton" text="Add" textFill="WHITE">
               <font>
                  <Font name="SansSerif Bold Italic" size="23.0" />
               </font>
            </Button>
            <Label fx:id="price" layoutX="28.0" layoutY="67.0" prefHeight="18.0" prefWidth="296.0" textFill="#df1a1a">
               <font>
                  <Font name="System Bold Italic" size="19.0" />
               </font></Label>
            <Label fx:id="exist" layoutX="205.0" layoutY="323.0" prefHeight="18.0" prefWidth="391.0" textFill="#d71111">
               <font>
                  <Font name="Arial Black" size="20.0" />
               </font>
            </Label>
            <Label fx:id="foldable" layoutX="44.0" layoutY="193.0" prefHeight="18.0" prefWidth="446.0" textFill="#b11818">
               <font>
                  <Font name="System Bold" size="19.0" />
               </font>
            </Label>
            <Button fx:id="Back" layoutX="618.0" layoutY="264.0" mnemonicParsing="false" onAction="#BackButton" style="-fx-background-radius: 50; -fx-background-color: #FF6666;" text="Back" textFill="#180f0f">
               <font>
                  <Font size="23.0" />
               </font>
            </Button>
         </children>
      </AnchorPane>
      <Label fx:id="errorMessage" alignment="CENTER" layoutX="74.0" layoutY="492.0" prefHeight="82.0" prefWidth="601.0" textFill="#aa1945">
         <font>
            <Font name="SansSerif Bold" size="18.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
